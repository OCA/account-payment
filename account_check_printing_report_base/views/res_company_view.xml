<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_company_form_view_inherited" model="ir.ui.view">
        <field name="name">Res Company Form View Inherited</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="check_printing_layout" string="Check printing layout">
                    <group name="check_layout">
                        <field name="check_layout_id" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
